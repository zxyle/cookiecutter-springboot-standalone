<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="{{ cookiecutter.basePackage }}.biz.sys.mapper.LoginLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="{{ cookiecutter.basePackage }}.biz.sys.entity.LoginLog">
        <result column="id" property="id" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="user_id" property="userId" />
        <result column="account" property="account" />
        <result column="ip" property="ip" />
        <result column="ua" property="ua" />
        <result column="msg" property="msg" />
        <result column="is_success" property="isSuccess" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        create_time, update_time, user_id, account, ip, ua, msg, is_success
    </sql>


    <select id="selectAll" resultType="{{ cookiecutter.basePackage }}.biz.sys.entity.LoginLog">
        SELECT * FROM sys_login_log
    </select>

</mapper>
